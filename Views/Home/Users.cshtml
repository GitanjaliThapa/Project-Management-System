@model List<Milestone3Project.Models.User>

<h2>Registered Users</h2>
<table>
    <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Email</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserId</td>
            <td>@user.Username</td>
            <td>@user.Email</td>
        </tr>
    }
</table>